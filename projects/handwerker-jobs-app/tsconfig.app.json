{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../out-tsc/app",
    "paths": {
      "@handwerker-jobs-core/*": ["./src/app/_core/*"],
      "@handwerker-jobs-shared/*": ["./src/app/_shared/*"],
      "@handwerker-jobs-app/*": ["./src/app/app/*"],
      "@handwerker-jobs-assets/*": ["./src/assets/*"],
      "@handwerker-jobs-environments/*": ["./src/environments/*"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
